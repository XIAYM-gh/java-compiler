package org.glamey.compiler;


public class Single {
    private String code;
    private String name;
    private OtherSingle otherSingle;

    public Single() {
        this.code = "glamey";
        this.name = "文曜";
        otherSingle = new OtherSingle();
    }

    public String getCode() {
        return otherSingle.other(this.code);
    }

    public String getName() {
        return name;
    }

    @Override
    public String toString() {
        return "UserObject{" +
                "code='" + code + '\'' +
                ", name='" + name + '\'' +
                '}';
    }
}